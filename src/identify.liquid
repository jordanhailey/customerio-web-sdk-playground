<section id="identify" class="py-5 px-4 sm:px-8 my-4 divide-y divide-gray-300 overflow-hidden rounded-lg bg-gray-100 shadow">
  <div class="py-5">
    <h2 class="pb-5 text-3xl sm:text-4xl font-semibold leading-7 text-gray-900">Identify a Profile</h2>
    <p class="mt-1 text-sm leading-6 text-gray-600">This data will be used to send an <code class="text-red-500 font-mono text-sm">identify</code> call to your Customer.io workspace.</p>
  </div>
  <div class="py-5">
    <form id="identify-form" action="/identify/#identify-form">
      <div class="space-y-12 my-4">
        <div class="border-b border-gray-900/10 pb-12">
          <div class="mt-6 grid grid-cols-1 gap-x-6 gap-y-8 sm:grid-cols-6">
            <div class="sm:col-span-4">
              <label for="id" class="block text-sm font-medium leading-6 text-gray-900"><code class="text-red-500 font-mono text-sm">id</code> (use either an ID or an email address)</label>
              <div class="mt-2">
                <div class="flex rounded-md shadow-sm ring-1 ring-inset ring-gray-300 focus-within:ring-2 focus-within:ring-inset focus-within:ring-teal-600 sm:max-w-md">
                  <input
                    disabled
                    type="text"
                    name="id"
                    id="id"
                    class="block flex-1 border-0 rounded-md py-1.5 px-1.5 text-gray-900 placeholder:text-gray-400 focus:ring-0 sm:text-sm sm:leading-6"
                    placeholder="identifier">
                </div>
              </div>
            </div>
            <div class="sm:col-span-4">
              <label for="email" class="block text-sm font-medium leading-6 text-gray-900"><code class="text-red-500 font-mono text-sm">email</code> (this should obviously by an email address)</label>
              <div class="mt-2">
                <div class="flex rounded-md shadow-sm ring-1 ring-inset ring-gray-300 focus-within:ring-2 focus-within:ring-inset focus-within:ring-teal-600 sm:max-w-md">
                  <input
                    disabled
                    type="email"
                    name="email"
                    id="email"
                    class="block flex-1 border-0 rounded-md py-1.5 px-1.5 text-gray-900 placeholder:text-gray-400 focus:ring-0 sm:text-sm sm:leading-6"
                    placeholder="email@example.com">
                </div>
              </div>
            </div>
            <div class="sm:col-span-4">
              <label for="customVariableName" class="block text-sm font-medium leading-6 text-gray-900">Custom Variable Name</label>
              <div class="mt-2">
                <div class="flex rounded-md shadow-sm ring-1 ring-inset ring-gray-300 focus-within:ring-2 focus-within:ring-inset focus-within:ring-teal-600 sm:max-w-md">
                  <input
                    disabled
                    type="text"
                    name="customVariableName"
                    id="customVariableName"
                    class="block flex-1 border-0 rounded-md py-1.5 px-1.5 text-gray-900 placeholder:text-gray-400 focus:ring-0 sm:text-sm sm:leading-6"
                    placeholder="Name of your custom variable">
                </div>
              </div>
            </div>
            <div class="sm:col-span-4">
              <label for="customVariableValue" class="block text-sm font-medium leading-6 text-gray-900">Custom Variable Value â€” an empty value will be passed as an empty string</label>
              <div class="mt-2">
                <div class="flex rounded-md shadow-sm ring-1 ring-inset ring-gray-300 focus-within:ring-2 focus-within:ring-inset focus-within:ring-teal-600 sm:max-w-md">
                  <input
                    disabled
                    type="text"
                    name="customVariableValue"
                    id="customVariableValue"
                    class="block flex-1 border-0 rounded-md py-1.5 px-1.5 text-gray-900 placeholder:text-gray-400 focus:ring-0 sm:text-sm sm:leading-6"
                    placeholder="Value for your Custom Variable">
                </div>
              </div>
            </div>
          </div>
        </div>
